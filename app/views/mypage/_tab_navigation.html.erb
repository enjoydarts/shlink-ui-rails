<!-- Tab Navigation -->
<div class="border-b border-gray-200 mb-6">
  <nav class="-mb-px flex space-x-8" aria-label="タブ">
    <!-- URL一覧タブ -->
    <button type="button" 
            data-action="click->mypage-tabs#switchTab"
            data-tab="urls"
            data-mypage-tabs-target="tab"
            class="mypage-tab-button whitespace-nowrap py-2 px-1 border-b-2 font-medium text-sm border-blue-500 text-blue-600 focus:outline-none focus:border-blue-700 focus:text-blue-800"
            aria-selected="true"
            aria-controls="urls-panel">
      <svg class="w-5 h-5 mr-2 inline" fill="none" stroke="currentColor" viewBox="0 0 24 24">
        <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M13.828 10.172a4 4 0 00-5.656 0l-4 4a4 4 0 105.656 5.656l1.102-1.102m-.758-4.899a4 4 0 005.656 0l4-4a4 4 0 00-5.656-5.656l-1.1 1.1"></path>
      </svg>
      URL一覧
    </button>

    <!-- 統計グラフタブ -->
    <button type="button"
            data-action="click->mypage-tabs#switchTab"
            data-tab="statistics"
            data-mypage-tabs-target="tab"
            class="mypage-tab-button whitespace-nowrap py-2 px-1 border-b-2 font-medium text-sm border-transparent text-gray-500 hover:text-gray-700 hover:border-gray-300 focus:outline-none focus:border-gray-300 focus:text-gray-700"
            aria-selected="false"
            aria-controls="statistics-panel">
      <svg class="w-5 h-5 mr-2 inline" fill="none" stroke="currentColor" viewBox="0 0 24 24">
        <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 19v-6a2 2 0 00-2-2H5a2 2 0 00-2 2v6a2 2 0 002 2h2a2 2 0 002-2zm0 0V9a2 2 0 012-2h2a2 2 0 012 2v10m-6 0a2 2 0 002 2h2a2 2 0 002-2m0 0V5a2 2 0 012-2h2a2 2 0 012 2v14a2 2 0 01-2 2h-2a2 2 0 01-2-2z"></path>
      </svg>
      全体統計
    </button>

    <!-- 個別URL分析タブ -->
    <button type="button"
            data-action="click->mypage-tabs#switchTab"
            data-tab="individual"
            data-mypage-tabs-target="tab"
            class="mypage-tab-button whitespace-nowrap py-2 px-1 border-b-2 font-medium text-sm border-transparent text-gray-500 hover:text-gray-700 hover:border-gray-300 focus:outline-none focus:border-gray-300 focus:text-gray-700"
            aria-selected="false"
            aria-controls="individual-panel">
      <svg class="w-5 h-5 mr-2 inline" fill="none" stroke="currentColor" viewBox="0 0 24 24">
        <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M13.828 10.172a4 4 0 00-5.656 0l-4 4a4 4 0 105.656 5.656l1.102-1.102m-.758-4.899a4 4 0 005.656 0l4-4a4 4 0 00-5.656-5.656l-1.1 1.1"></path>
      </svg>
      URL別分析
    </button>
  </nav>
</div>